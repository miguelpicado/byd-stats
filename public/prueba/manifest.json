{
    "name": "BYD Stats Debug",
    "short_name": "BYD Debug",
    "description": "Debug tool for BYD Stats - receives shared files",
    "start_url": "/prueba/",
    "scope": "/prueba/",
    "display": "standalone",
    "background_color": "#0f172a",
    "theme_color": "#ea0029",
    "orientation": "any",
    "icons": [
        {
            "src": "/favicon.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any maskable"
        },
        {
            "src": "/app_logo.png",
            "sizes": "192x192",
            "type": "image/png"
        }
    ],
    "share_target": {
        "action": "/prueba/",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url",
            "files": [
                {
                    "name": "file",
                    "accept": [
                        "*/*",
                        ".db",
                        ".sqlite",
                        ".sqlite3",
                        "application/x-sqlite3",
                        "application/vnd.sqlite3",
                        "application/octet-stream",
                        "application/binary"
                    ]
                }
            ]
        }
    },
    "file_handlers": [
        {
            "action": "/prueba/",
            "accept": {
                "application/x-sqlite3": [
                    ".db",
                    ".sqlite",
                    ".sqlite3"
                ],
                "application/octet-stream": [
                    ".db"
                ]
            }
        }
    ],
    "protocol_handlers": [
        {
            "protocol": "web+bydstats",
            "url": "/prueba/?file=%s"
        }
    ],
    "related_applications": [],
    "prefer_related_applications": false
}