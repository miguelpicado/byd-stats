{
    "name": "BYD Stats",
    "short_name": "BYD Stats",
    "description": "Analiza las estadísticas de tu vehículo BYD de forma local y privada",
    "start_url": "/",
    "scope": "/",
    "display": "standalone",
    "display_override": [
        "standalone"
    ],
    "background_color": "#0f172a",
    "theme_color": "#0f172a",
    "orientation": "any",
    "launch_handler": {
        "client_mode": "navigate-existing"
    },
    "icons": [
        {
            "src": "/app_icon_v2.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "any maskable"
        },
        {
            "src": "/app_icon_v2.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any maskable"
        }
    ],
    "share_target": {
        "action": "/",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url",
            "files": [
                {
                    "name": "file",
                    "accept": [
                        "*/*",
                        ".db",
                        ".sqlite",
                        ".sqlite3",
                        "application/x-sqlite3",
                        "application/vnd.sqlite3",
                        "application/octet-stream"
                    ]
                }
            ]
        }
    },
    "file_handlers": [
        {
            "action": "/",
            "accept": {
                "application/x-sqlite3": [
                    ".db",
                    ".sqlite",
                    ".sqlite3"
                ],
                "application/octet-stream": [
                    ".db"
                ]
            }
        }
    ],
    "related_applications": [],
    "prefer_related_applications": false
}